{
  "name": "next-front",
  "version": "0.1.0",
  "description": "nouvel extranet",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/ npm-run-all --parallel  test:watch open:src lint:watch",
    "start:dist": "node_modules/.bin/babel-node tools/distServer.js",
    "open:src": "node_modules/.bin/babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw --color webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "remove-dist": "rm -rf dist",
    "create-dist": "mkdir dist",
    "clean-dist": "node_modules/.bin/ npm-run-all remove-dist create-dist",
    "build:html": "node_modules/.bin/babel-node tools/buildHtml.js",
    "build:app": "node_modules/.bin/babel-node tools/build.js",
    "build:envConf": "node_modules/.bin/babel-node tools/buildEnvConf.js",
    "prebuild": "node_modules/.bin/ npm-run-all clean-dist build:envConf",
    "build": "node_modules/.bin/ npm-run-all build:app build:html",
    "build:zip": "node_modules/.bin/babel-node tools/buildZip.js",
    "test": "node_modules/.bin/mocha --reporter progress ./tools/testSetup.js \"./src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "postbuild": "npm run build:zip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/si2m/next/front.git"
  },
